{
	"usesTypeScript": false,
	"usesCssModule": false,
	"cssPreprocessor": "scss",
	"testLibrary": "None",
	"component": {
		"default": {
			"customTemplates": {
				"component": "templates/default/TemplateName.jsx",
				"style": "templates/default/TemplateName.scss",
				"index": "templates/default/index.js"
			},
			"path": "src/components",
			"withStyle": true,
			"withTest": false,
			"withStory": false,
			"withLazy": false,
			"withIndex": true
		},
		"page": {
			"customTemplates": {
				"component": "templates/page/TemplateName.jsx",
				"style": "templates/page/TemplateName.scss"
			},
			"path": "src/pages",
			"withLazy": false,
			"withStory": false,
			"withStyle": true,
			"withTest": false
		}
	}
}
